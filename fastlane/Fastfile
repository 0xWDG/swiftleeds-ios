default_platform(:ios)

platform :ios do
  desc "Build SwiftLeeds scheme"
  lane :build do
    build_app(
      project: "SwiftLeeds.xcodeproj",
      configuration: "Debug",
      scheme: "SwiftLeeds",
      silent: true,
      clean: true,
      skip_archive: true,
      skip_codesigning: true
    )
  end
end
